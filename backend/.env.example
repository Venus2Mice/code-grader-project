# Backend Environment Variables Template
# Copy this file to .env and update with your values

# Database connection
# For Docker: use service name 'postgres'
# For local: use 'localhost'
DATABASE_URL=postgresql://postgres:your_password@postgres:5432/code_grader

# Flask configuration
SECRET_KEY=your_secret_key_here

# Flask settings
FLASK_APP=run.py
FLASK_DEBUG=1

# RabbitMQ connection
# For Docker: use service name 'rabbitmq'
# For local: use 'localhost'
RABBITMQ_HOST=rabbitmq
