# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_QUEUE=grading_queue

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/code_grader?sslmode=disable

# Backend API Configuration
BACKEND_API_URL=http://localhost:5000

# Docker Configuration
DOCKER_IMAGE=cpp-grader-env
CONTAINER_POOL_SIZE=3

# Resource Limits
DEFAULT_TIME_LIMIT=1000
DEFAULT_MEMORY_LIMIT=256000

# Temp Directory
TEMP_DIR=/tmp/grader
